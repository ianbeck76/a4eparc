    <h3 style="color:dimgray" class="col-xs-11">@Html.DisplayFor(m => m.Questions.FirstOrDefault(q => q.Code == "11").Description)</h3>
    <h3 style="font-weight:bolder;color:black" class="col-xs-1">@Model.AnswerString.Substring(20, 1)</h3>
