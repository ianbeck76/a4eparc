<div class="row">
    <div class="col-sm-4 col-sm-offset-4">

        <p><br /></p>

        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Options</h3>
            </div>
            <div class="panel-body">

                    <p class="text-center">
                        @if (Request.Url.AbsoluteUri.ToLower().Contains("exemplar"))
                        {
                            @Html.ActionLink("Begin Survey", "PageOne", "Survey", null, new { @class = "btn btn-primary btn-block btn-lg" })
                        }
                        else
                        {
                            @Html.ActionLink("Begin Survey", "GlsOne", "Survey", null, new { @class = "btn btn-primary btn-block btn-lg" })
                        }
                    </p>
            
                    @if (Context.User.IsInRole("IsViewer"))
                    {
                          <p class="text-center">
                            <a href="@ViewBag.BrowseResultsLink" class="btn btn-primary btn-block btn-lg">View Client Data</a>
                        </p>
                    }
                
                  @*  @if (Context.User.IsInRole("IsSuperAdmin"))
                    {
                        <p class="text-center">
                            @Html.ActionLink("View WebService Data", "Index", "WebServiceResults", null, new {@class = "btn btn-primary btn-block btn-lg"})
                        </p>
                    }*@

            </div>
            <div class="panel-footer text-center">
                Please choose a option from above.
            </div>
        </div>

    </div>
</div>